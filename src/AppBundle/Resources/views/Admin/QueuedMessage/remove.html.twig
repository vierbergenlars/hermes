{% block content %}
    <div class="panel-heading">
        <h3 class="panel-title">{{ icon('paper-plane-o') }} {{ 'admin.queuedmessage.delete.title'|trans({'%id%': message.id, '%to%': message.toAddress, '%subject%': message.subject, '%from%': message.fromAddress}) }}</h3>
    </div>
    <div class="panel-body">
        {{ form(form) }}
    </div>
{% endblock %}
